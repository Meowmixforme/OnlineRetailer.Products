// Example layout. Actual configuration would be inside of secrets
{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ProductsContext": "<YOUR_AZURE_SQL_CONNECTION_STRING>"
  },
  "Auth": {
    "Authority": "https://<YOUR_AUTH0_TENANT>.eu.auth0.com",
    "Audience": "https://<YOUR_API_IDENTIFIER>"
  },
  "WebServices": {
    "UnderCutters": {
      "BaseURL": "http://undercutters.azurewebsites.net/"
    }
  }
}
    
